<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <title>$WikiTitle | {$Group} / {$Title} $ActionTitle</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">

    <!-- Le styles -->
    <link href="$FarmPubDirUrl/css/screen.css" rel="stylesheet">

    <link rel='stylesheet' href='$SkinDirUrl/pmwiki.css' type='text/css' />

    <!--HTMLHeader-->

    <script src="$FarmPubDirUrl/javascripts/vendor/jquery-1.8.3.min.js"></script>

    <!-- HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
      <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->

    <!-- Fav and touch icons -->
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="$FarmPubDirUrl/images/bootstrap/ico/apple-touch-icon-144-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="$FarmPubDirUrl/images/bootstrap/ico/apple-touch-icon-114-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="$FarmPubDirUrl/images/bootstrap/ico/apple-touch-icon-72-precomposed.png">
    <link rel="apple-touch-icon-precomposed" href="$FarmPubDirUrl/images/bootstrap/ico/apple-touch-icon-57-precomposed.png">
    <link rel="shortcut icon" href="$FarmPubDirUrl/images/bootstrap/ico/favicon.png">

  </head>

  <body>

  <!--PageHeaderFmt-->

    <div id="wikihead" class="navbar navbar-inverse navbar-fixed-top">
      <div class="navbar-inner">
        <div class="container-fluid">
          <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </a>
          <a id="wikilogo" class="brand" href="{$ScriptUrl}"><img style="height:1em;" src='$PageLogoUrl' alt='$WikiTitle' border='0' /> $WikiTitle</a>

          <div class="nav-collapse collapse">

            <div class="pull-right">

              <form class="navbar-search pull-left" action="{$ScriptUrl}">
                <span class='headnav'><a href='{$ScriptUrl}/$[{$Group}/RecentChanges]' accesskey='$[ak_recentchanges]'>$[Recent Changes]</a> -</span>
                <input type="text" name='q' value='' class="search-query span2" placeholder="Search">
                <button type='submit' class='btn'>$[Go]</button>
                <input type='hidden' name='n' value='{$FullName}' />
                <input type='hidden' name='action' value='search' />
              </form>

            </div>

            <ul class="nav">
              <li class="active"><a href="{$ScriptUrl}/Main/HomePage">Home</a></li>
              <li><a href="#about">About</a></li>
              <li><a href="#contact">Contact</a></li>
            </ul>

          </div><!--/.nav-collapse -->
        </div>
      </div>
    </div>

    <!--/PageHeaderFmt-->

    <div id="wikimid" class="container-fluid">
      <div class="row-fluid">

        <!--PageLeftFmt-->
        <div id="wikileft" class="span3">
          <div class="well sidebar-nav">
            <!--wiki:{$Group}.SideBar {$SiteGroup}.SideBar-->
          </div><!--/.well -->
        </div><!--/span-->
        <!--/PageLeftFmt-->

        <div id="wikibody" class="span9">

          <!--PageActionFmt-->
          <div id="wikicmds">
            <!--wiki:{$Group}.PageActions {$SiteGroup}.PageActions-->
          </div>

          <!--PageTitleFmt-->
          <div id="wikititle">
            <div class="pagegroup">
              <a href='{$ScriptUrl}/{$Group}'>{$Group}</a> /
            </div>
            <h1 class="pagetitle">{$Title}</h1>
          </div>

          <!--PageText-->

        </div><!--/span-->
      </div><!--/row-->

<!--PageFooterFmt-->
      <hr />

      <footer>
        <div id='wikifoot'>
          <div class='footnav'>
            <a rel="nofollow" href='{$PageUrl}?action=edit'>$[Edit]</a> -
            <a rel="nofollow" href='{$PageUrl}?action=diff'>$[History]</a> -
            <a rel="nofollow" href='{$PageUrl}?action=print' target='_blank'>$[Print]</a> -
            <a href='{$ScriptUrl}/$[{$Group}/RecentChanges]'>$[Recent Changes]</a> -
            <a href='{$ScriptUrl}/$[{$SiteGroup}/Search]'>$[Search]</a>
          </div>
          <div class='lastmod'>$[Page last modified on {$LastModified}]</div>
        </div>
      </footer>
<!--/PageFooterFmt-->

    </div><!--/.fluid-container-->

    <!-- Le javascript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    <script src="$FarmPubDirUrl/javascripts/bootstrap/bootstrap.min.js"></script>

    <!--HTMLFooter-->
  </body>
</html>
